{
    "name": "elmer-cli",
    "version": "1.1.31",
    "description": "cli tool for elmer-ui-app",
    "typings": "lib/index.d.ts",
    "main": "lib/index.js",
    "module": "lib/index.js",
    "bin": {
        "elmer-cli": "./bin/index.js"
    },
    "scripts": {
        "test": "node ./bin/index.js -v",
        "dev": "node ./bin/index.js init",
        "start": "node ./bin/index.js start -t ./public/index.html -e ./public/index.ts",
        "build": "node ./bin/index.js build",
        "uv": "node ./bin/index.js uv um=c",
        "path": "node ./bin/index.js path",
        "ver": "node ./bin/index.js version",
        "deploy": "ts-node ./src/deploy/index.ts",
        "static": "node ./bin/index.js static"
    },
    "author": "250933400@qq.com",
    "license": "ISC",
    "elmer-cli": {
        "hash": true,
        "host": "192.168.0.102",
        "devtool": "source-map",
        "optimization": false,
        "port": "3003",
        "output": {
            "filename": "[name].min.js",
            "publicPath": "",
            "chunkFilename": "chunks/[name]_[hash:8]_[id].min.js"
        },
        "template": "./public/index.html",
        "entry": "./public/index.ts"
    },
    "browserslist": [
        "last 2 version",
        "> 1%",
        "iOS >= 7",
        "Android > 4.1",
        "Firefox > 20"
    ],
    "dependencies": {
        "@babel/core": "^7.10.4",
        "@types/node": "^14.0.14",
        "@types/webpack-dev-server": "^3.11.0",
        "autoprefixer": "^10.2.5",
        "babel-loader": "^8.1.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-preset-env": "^1.7.0",
        "clean-webpack-plugin": "^0.1.19",
        "cli-color": "^2.0.0",
        "colors": "^1.4.0",
        "commander": "^4.1.0",
        "copy-webpack-plugin": "^5.0.3",
        "css-loader": "^5.1.2",
        "elmer-common": "^0.1.10",
        "elmer-loader": "^1.0.6",
        "elmer-virtual-dom": "^1.0.10",
        "file-loader": "^6.2.0",
        "global": "^4.4.0",
        "html-webpack-plugin": "^4.3.0",
        "ignore-loader": "^0.1.2",
        "inquirer": "^7.0.3",
        "less": "^4.1.1",
        "less-loader": "^8.0.0",
        "mini-css-extract-plugin": "^1.3.8",
        "postcss": "^8.1.0",
        "postcss-loader": "^5.2.0",
        "progress-bar-webpack-plugin": "^2.1.0",
        "shelljs": "^0.8.3",
        "style-loader": "^2.0.0",
        "superagent": "^5.2.1",
        "ts-loader": "^7.0.5",
        "tslint": "^6.1.2",
        "tslint-loader": "^3.5.4",
        "typescript": "^3.9.7",
        "url-loader": "^4.1.1",
        "webpack": "^5.25.0",
        "webpack-cli": "^4.5.0",
        "webpack-dashboard": "^3.3.1",
        "webpack-dev-server": "^3.11.2",
        "webpack-merge": "^5.7.3"
    }
}
