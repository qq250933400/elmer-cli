        "babel-preset-env": "^1.7.0",
        "clean-webpack-plugin": "^0.1.19",
        "cli-color": "^2.0.0",
        "colors": "^1.4.0",
        "commander": "^4.1.0",
        "copy-webpack-plugin": "^5.0.3",
        "elmer-common": "^0.1.10",
        "elmer-loader": "^1.0.2",
        "elmer-virtual-dom": "^1.0.10",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "global": "^4.4.0",
