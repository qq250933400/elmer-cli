        "npm": "^6.14.9",
        "progress-bar-webpack-plugin": "^2.1.0",
        "shelljs": "^0.8.3",
        "superagent": "^5.2.1",
        "ts-loader": "^7.0.5",
        "tslint": "^6.1.2",
        "tslint-loader": "^3.5.4",
        "typescript": "^3.9.7",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "webpack": "^4.16.0",
